# .1.3.6.1.4.1.476.1.42.3.9.20.1.30.1.2.1.4644 deg C


def discover_liebert_temp_fluid(section):
    yield from ((name, {}) for name in section.readings if "Set Point" in name)


def check_liebert_temp_fluid(item, params, parsed):
    reading = parsed.readings.get(item)
    if reading is None:
        return

    yield check_temperature(
        reading=reading,
        params=params,
        unique_name="check_liebert_temp_fluid.%s" % item,
        dev_levels=parsed.upper_device_levels,
        dev_levels_lower=parsed.lower_device_levels,
    )


check_info["liebert_temp_fluid"] = {
    "inventory_function": discover_liebert_temp_fluid,
    "check_function": check_liebert_temp_fluid,
    "service_description": "%s",
    "group": "temperature",
    "has_perfdata": True,
}
